{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "plugins": ["react", "import"],
  "rules": {
    "react/react-in-jsx-scope": "off", // Example rule customization
    "import/no-unresolved": "error"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
}
